<div class="container-fluid container-fluid-custom py-5 mb-5">
    <div class="row mb-4">
        <div class="col">
            <nav aria-label="breadcrumb">
                <ol class="breadcrumb">
                    <li class="breadcrumb-item"><a href="/dashboard">Dashboard</a></li>
                    <li class="breadcrumb-item">Search</li>
                </ol>
            </nav>
        </div>
    </div>
    <div class="row">
        <% if(searchResults.length > 0) { for(let i = 0 ; i<searchResults.length ; i++){ %>
            <div class="col-sm-3 mb-4">
                <div class="card border-primary" style="min-height: 210px;">
                    <a href="/dashboard/item/<%= searchResults[i]._id %>" class="card-body text-decoration-none">
                        <h5 class="card-title"><%= searchResults[i].title %></h5>
                        <p class="card-text"><%= searchResults[i].body %></p>
                    </a>
                </div>
            </div>
        <% }} else {%>
            <h1>No items found.</h1>
        <% } %>
    </div>
</div>